  "description": "vue3 webpack5 typescript",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve  --env mode=local env=dev --config ./build/webpack.dev.config.js",
    "test": "webpack serve  --env mode=local env=test --config ./build/webpack.dev.config.js",
    "pre": "webpack serve  --env mode=local env=pre --config ./build/webpack.dev.config.js",
    "build:dev": "webpack  --env mode=server env=dev --config ./build/webpack.build.config.js",
    "build:test": "webpack  --env mode=server env=test --config ./build/webpack.build.config.js",
    "build:pre": "webpack  --env mode=server env=pre --config ./build/webpack.build.config.js",
    "build:prod": "webpack  --env mode=server env=prod --config ./build/webpack.build.config.js"
  },
